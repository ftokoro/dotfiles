set number
syntax enable
inoremap <silent> jj <ESC>
colorscheme deus
set t_Co=256
set shiftwidth=2

set tabstop=2

set expandtab

set cursorline
set cursorcolumn

set showmatch 

set matchtime=1

set smartindent

nnoremap j gj
nnoremap k gk

set ignorecase

set smartcase

set wrapscan

set hlsearch

set nowritebackup
set nobackup
set noswapfile

